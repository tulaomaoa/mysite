			// if( isShow == 'no'){
			//     show()
			//     $input.focus();
			// }else{
			// 	if($input.val()==""){
			// 		hide()
			// 	}else{
			// 		searchbox.search($input.val())
			// 	}
			// }
			if ($input.val() != "") {
				searchbox.search($input.val())
			} else {
				$input.focus();
			}
			event.stopPropagation();
			return false;
		})

		$('#nav, #sub-nav').on('click', '.search-area', function (event) {
			event.stopPropagation();
			return false;
		})

