	// 设置“回到顶部”按钮的显隐状态
	function setGo2TopBtnState () {
		h = $(window).height();
		t = $(document).scrollTop();
		if (t >= 768) {
			$('#backTop').show();
			$('#js-elevator-weixin').removeClass('no-goto');
		} else {
			$('#backTop').hide();
			$('#js-elevator-weixin').addClass('no-goto');
		}
	}
