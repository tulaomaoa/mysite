define(function(require){require("/static/moco/v1.0/dist/js/moco.min.js?t=06111806"),$.get("/common/adver-getadver",function(a){if(a=JSON.parse(a),0==a.result){var g=a.data.globalRightFloat,c=a.data.globalTopBanner,v=a.data.readTopBanner,h=a.data.courseIndexBanner,k=a.data.coursechapterlistRight,w=a.data.courseVideoRight,j=a.data.courseVideoWendalistRight;if(g.length&&(R=g[0],moco.initAdv("#globalRightFloat",{image:"//img.mukewang.com/"+R.pic+"-160-160.jpg",link:R.links,advid:R.id,closebtn:!0})),c.length&&(R=c[0],moco.initAdv("#globalTopBanner",{image:"//img.mukewang.com/"+R.pic+".jpg",link:R.links,advid:R.id})),v.length){var R=v[0];moco.initAdv("#readTopBanner",{image:"//img.mukewang.com/"+R.pic+".jpg",link:R.links,advid:R.id})}if(h.length){var R=h[0];moco.initAdv("#courseIndexBanner",{image:"//img.mukewang.com/"+R.pic+".jpg",link:R.links,advid:R.id,animateShow:"fadeIn"})}else $("#courseIndexBanner").append('<img src="/static/img/course/course-top.png" style="cursor:default">').show();if(k.length){var R=k[0];moco.initAdv("#coursechapterlistRight",{image:"//img.mukewang.com/"+R.pic+".jpg",link:R.links,advid:R.id,animateShow:"fadeIn"})}if(w.length){var R=w[0];moco.initAdv("#courseVideoRight",{image:"//img.mukewang.com/"+R.pic+".jpg",link:R.links,advid:R.id,animateShow:"fadeIn"})}if(j.length){var R=j[0];moco.initAdv("#courseVideoWendalistRight",{image:"//img.mukewang.com/"+R.pic+".jpg",link:R.links,advid:R.id,animateShow:"fadeIn"})}}})});